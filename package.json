{
  "name": "cmp-helper",
  "displayName": "cmp-helper",
  "description": "cmp 打包同步文件助手",
  "version": "0.3.1",
  "publisher": "cmpHelper",
  "author": {
    "name": "shuqi"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/CmpHelper.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.cmpPackage",
        "title": "CMP编译打包"
      },
      {
        "command": "extension.wechatPackage",
        "title": "Wechat编译打包"
      },
      {
        "command": "extension.chToUnicode",
        "title": "中文转Unicode"
      },
      {
        "command": "extension.unicodeToCh",
        "title": "Unicode转中文"
      },
      {
        "command": "extension.propertiesToJs",
        "title": "Properties转JS文件"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.cmpPackage",
          "group": "1_modification@1"
        },
        {
          "command": "extension.wechatPackage",
          "group": "1_modification@2"
        },
        {
          "command": "extension.chToUnicode",
          "group": "1_modification@3",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.unicodeToCh",
          "group": "1_modification@4",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.propertiesToJs",
          "group": "1_modification@5",
          "when": "resourceExtname == .properties"
        }
      ],
      "editor/title/context": [
        {
          "command": "extension.chToUnicode",
          "group": "1_modification@2",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.unicodeToCh",
          "group": "1_modification@3",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.propertiesToJs",
          "group": "1_modification@4",
          "when": "resourceExtname == .properties"
        }
      ],
      "editor/context": [
        {
          "command": "extension.chToUnicode",
          "group": "1_modification@2",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.unicodeToCh",
          "group": "1_modification@3",
          "when": "resourceExtname == .properties"
        },
        {
          "command": "extension.propertiesToJs",
          "group": "1_modification@4",
          "when": "resourceExtname == .properties"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "cmp helper Configuration",
      "properties": {
        "seeyon.cmp-helper": {
          "type": "object",
          "default": {},
          "description": "配置m3 tools目录、v5运行环境目录、代码目录"
        }
      }
    }
  },
  "keywords": [
    "cmp",
    "cmphelper",
    "seeyon",
    "v5"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "compressing": "^1.4.0",
    "date-format": "^3.0.0",
    "request": "^2.88.0"
  },
  "icon": "images/logo.png",
  "bugs": {
    "email": "shuqiyige@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shuqiyige/cmp-helper.git"
  },
  "homepage": "https://github.com/shuqiyige/cmp-helper/blob/master/README.md"
}